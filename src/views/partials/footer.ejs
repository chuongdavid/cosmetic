            if(r){
                const id = e.target.dataset.id;
                console.log('id',id)
                $.ajax({
                    type: 'DELETE',
                    url: "/user/delete/" + id,
                    success: function (response) {
                        
                        window.location.reload()
                    },
                    error: function (err){
                        console.log(err)
                    }
                })
            }
            
        })
    })
