            if(r){
                const id = e.target.dataset.id;
                $.ajax({
                    type: 'DELETE',
                    url: "/product/delete/" + id,
                    success: function (response) {
                        
                        window.location.reload()
                    },
                    error: function (err){
                        console.log(err)
                    }
                })
            }
            
        })

        ///delete user
        $('.delete-user').on('click', (e) => {
